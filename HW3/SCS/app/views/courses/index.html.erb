<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Dept</th>
      <th>Crhr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <!-- Search  Course-->
<%= form_tag(search_courses_url, method: "get") do %>
    <%= label_tag(:query, "Search for a Course:") %>
    <%= text_field_tag(:query) %>
    <%= submit_tag("Search") %>
<% end %>


  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.number %></td>
        <td><%= course.dept %></td>
        <td><%= course.crHr %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

